    if(sessionStorage.getItem("product2")!=null){items += "<li>"+ sessionStorage.getItem("product2") +"</li>";}
    if(sessionStorage.getItem("product3")!=null){items += "<li>"+ sessionStorage.getItem("product3") +"</li>";}
    if(sessionStorage.getItem("product4")!=null){items += "<li>"+ sessionStorage.getItem("product4") +"</li>";}
    if(sessionStorage.getItem("product5")!=null){items += "<li>"+ sessionStorage.getItem("product5") +"</li>";}
